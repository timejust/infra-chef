CREATE TABLE IF NOT EXISTS `calendar_event` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `event_id` varchar(64) NOT NULL,
  `user_id` varchar(64) DEFAULT NULL,
  `start` int(10) DEFAULT NULL,
  `end` int(10) DEFAULT NULL,
  `lat` double NOT NULL,
  `lng` double NOT NULL,
  `location` varchar(128) DEFAULT NULL,
  `summary` text,
  `description` text,
  `created_at` int(10) DEFAULT NULL,
  `updated_at` int(10) DEFAULT NULL,
  PRIMARY KEY (`id`,`event_id`) USING BTREE,
  UNIQUE KEY `eventId` (`event_id`) USING BTREE,
  KEY `userId` (`user_id`),
  KEY `dateRange` (`start`,`end`),
  KEY `position` (`lat`,`lng`) USING BTREE
) ENGINE=MyISAM AUTO_INCREMENT=2018500 DEFAULT CHARSET=utf8